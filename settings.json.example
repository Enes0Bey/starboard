"token": "bot token for authentication",
"serverID": "server/guild ID that bot will reside in. for example: 693277318496920994",
"channelID": "channel ID that bot posts in. for example: 461248569698208129",
"reactionEmoji": "emoji name that bot listens for. for example: moon2SMUG",
"threshold": threshold for how many reactions it takes to post to board. example: 15

